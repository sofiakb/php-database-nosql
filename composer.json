{
    "name": "sofiakb/php-database-nosql",
    "description": "A library to use nosql database",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sofiakb\\Database\\NoSQL\\": "src/",
            "Sofiakb\\Database\\Tests\\Models\\": "tests/Models/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "Sofiakb",
            "email": "contact.sofiakb@gmail.com",
            "homepage": "https://sofianeakbly.fr",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "contact.sofiakb@gmail.com"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "ext-json": "*",
        "sofiakb/php-filesystem": "^0.0.4",
        "sofiakb/php-support": "^0.0.10",
        "tightenco/collect": "^8.34"
    },
    "bin": [
        "nsqldb"
    ],
    "scripts": {
        "doc": "phpdoc --directory . --target docs --title 'php-database-nosql documentation' --ignore vendor -vv"
    },
    "version": "dev-main",
    "time": "2021-09-02"
}